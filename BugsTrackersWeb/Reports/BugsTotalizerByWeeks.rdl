  <LeftMargin>1in</LeftMargin>
  <DataSets>
    <DataSet Name="BugTracker">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     
ProjectCategories.CategoryName,
Count(Issues.DateCreated) as Nr, 
DATEPART(yy, Issues.DateCreated) AS An,
'Tr' + DATENAME(qq, Issues.DateCreated) AS Trimestru,
'Luna ' + DATENAME(mm, Issues.DateCreated) AS Luna,
'Sapt.' + DATENAME(ww, Issues.DateCreated) AS Saptam,
ProjectMilestones.MilestoneName
FROM         ProjectCategories INNER JOIN
                      Issues ON ProjectCategories.CategoryId = Issues.IssueCategoryId INNER JOIN
                      Users ON Issues.IssueCreatorId = Users.UserId INNER JOIN
                      ProjectMilestones ON Issues.IssueMilestoneId = ProjectMilestones.MilestoneId
GROUP BY DATEPART(yy, Issues.DateCreated),DATENAME(qq, Issues.DateCreated), DATENAME(mm, Issues.DateCreated),
 DATENAME(ww, Issues.DateCreated) ,ProjectCategories.CategoryID ,ProjectCategories.CategoryName ,ProjectMilestones.MilestoneName,ProjectCategories.ProjectId
HAVING  ProjectCategories.ProjectId = 2 AND  ProjectCategories.CategoryID = 2
ORDER BY DATEPART(yy, Issues.DateCreated) , CONVERT(int,DATENAME(ww, Issues.DateCreated))</CommandText>
        <DataSourceName>BugTracker</DataSourceName>
      </Query>
      <Fields>
        <Field Name="CategoryName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>CategoryName</DataField>
        </Field>
        <Field Name="Nr">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Nr</DataField>
        </Field>
        <Field Name="An">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>An</DataField>
        </Field>
        <Field Name="Trimestru">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Trimestru</DataField>
        </Field>
        <Field Name="Luna">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Luna</DataField>
        </Field>
        <Field Name="Saptam">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Saptam</DataField>
        </Field>
        <Field Name="MilestoneName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>MilestoneName</DataField>
        </Field>
      </Fields>
    </DataSet>

          <DataField>Data</DataField>
        </Field>
        <Field Name="DisplayName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>DisplayName</DataField>
        </Field>
        <Field Name="IssueId">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>IssueId</DataField>

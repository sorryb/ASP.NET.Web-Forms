  <DataSets>
    <DataSet Name="BugTracker">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select * from SelectTaskByID where issueID = @issueID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@issueID">

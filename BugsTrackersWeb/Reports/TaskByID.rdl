    <DataSet Name="Comments">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     IssueComments.Comment,CONVERT(varchar(10), IssueComments.DateCreated, 104) +' '+ CONVERT(varchar(10),IssueComments.DateCreated, 108) AS Data, Users.DisplayName, IssueComments.IssueId
FROM         IssueComments INNER JOIN
                      Users ON IssueComments.UserId = Users.UserId
WHERE     (IssueComments.IssueId = @issueID)
ORDER BY Data</CommandText>
        <QueryParameters>
          <QueryParameter Name="@issueID">
            <Value>=Parameters!issueID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>BugTracker</DataSourceName>
      </Query>
